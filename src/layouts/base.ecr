  <script>
  $(document).ready(function(){
    // Show full page LoadingOverlay
    //$.LoadingOverlay("show");

    //Close the LoadingOverlay
    //$.LoadingOverlay("hide");

    $(document).on("click","[data-load-link='load']",async function(e){
      e.preventDefault();

      // $.LoadingOverlay("show");

      let link = $(this).attr("data-link");
      await App.load_page(link).catch(message => App.show_error(message));
      window.history.pushState({}, null, "/");

      // $.LoadingOverlay("hide");
    });

    App.components();

    // Charts
    var data = {
      labels: ['A', 'B', 'C', 'D', 'E', 'F'],
      series: [
        [15, 14, 13, 17, 15, 20]
      ]
    };

    var options = {
      seriesBarDistance: 15
    };

    var responsiveOptions = [
      ['screen and (min-width: 641px) and (max-width: 1024px)', {
        seriesBarDistance: 10,
        axisX: {
          labelInterpolationFnc: function (value) {
            return value;
          }
        }
      }],
      ['screen and (max-width: 640px)', {
        seriesBarDistance: 5,
        axisX: {
          labelInterpolationFnc: function (value) {
            return value[0];
          }
        }
      }]
    ];
    
    new Chartist.Bar(".ct-chart-1", data, options, responsiveOptions);
    new Chartist.Bar(".ct-chart-2", data, options, responsiveOptions);
    new Chartist.Bar(".ct-chart-3", data, options, responsiveOptions);    

  });
  </script>
